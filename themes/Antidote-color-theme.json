{
  "name": "Antidote",
  "type": "dark",
  "colors": {
    "activityBar.background": "#111111",
    "activityBarBadge.background": "#66be84",
    "activityBarBadge.foreground": "#252526",
    "activityBar.foreground": "#dadada",
    "badge.background": "#66be84",
    "badge.foreground": "#252526",
    "editorLineNumber.activeForeground": "#ffffff",
    "editor.background": "#181818",
    "editor.foreground": "#b3b3b3",
    "editor.lineHighlightBackground": "#333",
    "editor.lineHighlightBorder": "#333",
    "editorError.foreground": "#da6a48",
    "editorBracketMatch.border": "#343434", // border color of matching bracket
    "editorBracketMatch.background": "#343434", // background color of matching bracket
    "editor.wordHighlightBackground": "#071f28", // background color of elements
    "editor.findMatchBackground": "#071f28", // background color of current match
    "editor.findMatchBorder": "#071f28", // border color of current match
    "editor.findMatchHighlightBackground": "#000000", // background of inactive "find" matches
    "editor.findMatchHighlightBorder": "#000000", // border of inative "find" matches
    "editor.selectionHighlightBackground": "#000000", // highlights background of inactive matches
    "input.foreground": "#CCCCCC",
    "input.placeholderForeground": "#CCCCCC",
    "list.inactiveSelectionBackground": "#1f1f1f",
    "list.activeSelectionBackground": "#5c3999",
    "list.focusOutline": "#252526",
    "panel.border": "#343434",
    "panel.background": "#3a3636",
    "panelSectionHeader.background": "#5f5f5f",
    "panelSectionHeader.foreground": "#cccccc",
    "panelTitle.inactiveForeground": "#999999",
    "settings.numberInputForeground": "#CCCCCC",
    "sideBar.background": "#1C1C1C",
    "sideBar.foreground": "#b3b3b3",
    "sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#2d2d2d",
    "statusBar.background": "#2d2d2d",
    "statusBar.noFolderBackground": "#4C4C4C",
    "statusBar.debuggingBackground": "#5c3999",
    "statusBar.border": "#343434",
    "tab.activeBorderTop": "#66be84",
    "terminal.background":"#1C1C1C",
    "terminal.foreground":"#CCCCCC",
    "terminalCursor.background":"#aca59c",
    "terminalCursor.foreground":"#aca59c",
    "terminal.ansiBlack": "#282a36",
    "terminal.ansiRed": "#ff5c57",
    "terminal.ansiGreen": "#66be84",
    "terminal.ansiYellow": "#f3f99d",
    "terminal.ansiBlue": "#57c7ff",
    "terminal.ansiMagenta": "#ff6ac1",
    "terminal.ansiCyan": "#9aedfe",
    "terminal.ansiWhite": "#f1f1f0",
    "terminal.ansiBrightBlack": "#686868",
    "terminal.ansiBrightRed": "#ff5c57",
    "terminal.ansiBrightGreen": "#66be84",
    "terminal.ansiBrightYellow": "#f3f99d",
    "terminal.ansiBrightBlue": "#57c7ff",
    "terminal.ansiBrightMagenta": "#ff6ac1",
    "terminal.ansiBrightCyan": "#9aedfe",
    "terminal.ansiBrightWhite": "#eff0eb"
  },
  "tokenColors": [
    {
      "scope": [
        "entity.name.function",
        "meta.object-literal.key"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.language.this",
        "storage.type",
        "entity.name.type",
        "entity.other.attribute-name",
        "punctuation.definition.comment",
        "comment.block",
        "keyword.control.default",
        "meta.object-literal.key",
        "variable.other.object",
        "comment.line.double-slash",
        "punctuation.definition.comment",
        "punctuation.definition",
        "punctuation.support",
        "variable.other.readwrite",
        "punctuation.support.type.property-name.begin",
        "punctuation.definition.block.tag",
        "comment.line.number-sign",
        "constant.language.null"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string.template.js.jsx"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "storage.modifier",
        "constant.language.boolean"
      ],
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "support.type.property-name",
        "entity.name.function",
        "punctuation.definition.heading.markdown",
        "entity.name.type",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#789ad8"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attributes"
      ],
      "settings": {
        "foreground": "#b47abe"
      }
    },
    {
      "scope": [
        "meta.tag.attributes",
        "constant.language.boolean",
        "constant.language.null",
        "meta.tag.without-attributes"
      ],
      "settings": {
        "foreground": "#ebf2ec"
      }
    },
    {
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "variable.other.object.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#dadada"
      }
    },
    {
      "scope": [
        "comment.line.double-slash",
        "punctuation.definition.comment",
        "comment.block",
        "punctuation.definition",
        "punctuation.support",
        "variable.other.readwrite",
        "punctuation.support.type.property-name.begin",
        "punctuation.definition.block.tag",
        "comment.line.number-sign"
      ],
      "settings": {
        "foreground": "#6d6d6d"
      }
    },
    {
      "scope": [
        "keyword.control.import",
        "keyword.control.flow",
        "keyword.other.special-method",
        "support.constant.property-value",
        "keyword.other.using",
        "keyword.other.package",
        "meta.import",
        "keyword.operator.arithmetic",
        "storage.type.function.arrow",
        "keyword.operator.comparison",
        "keyword.operator.increment-decrement",
        "keyword.operator.relational",
        "markup.fenced_code.block",
        "keyword.operator.assignment",
        "punctuation.separator",
        "meta.link.inline.markdown",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#c77056"
      }
    },
    {
      "scope": [
        "support.type.property-name",
        "variable.parameter",
        "meta.object-literal.key",
        "storage.type",
        "support.type.property-name",
        "keyword.other.get",
        "keyword.other.set"
      ],
      "settings": {
        "foreground": "#b994fa"
      }
    },
    {
      "scope": [
        "punctuation.definition.block",
        "punctuation.section.embedded.end",
        "punctuation.section.embedded.begin",
        "support.variable.property",
        "variable.other.readwrite",
        "variable.other.constant",
        "entity.name.type.alias.ts",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#e9c076"
      }
    },
    {
      "scope": [
        "punctuation.definition.variable.less",
        "variable.parameter.js.jsx",
        "variable.other.less",
        "variable.scss",
        "keyword.type",
        "entity.name.type.namespace",
        "variable.other.readwrite.alias",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#70b6aa"
      }
    },
    {
      "scope": [
        "punctuation.definition.keyword.scss",
        "keyword.control.at-rule.mixin.scss",
        "keyword.control.at-rule.include.scss",
        "support.constant.handlebars",
        "storage.modifier",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#66be84"
      }
    }
  ]
}